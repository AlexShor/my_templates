{
    "name": "My tamplates",
    "description": "Templates to insert into fields",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [
        "scripting", 
        "activeTab", 
        "tabs", 
        "storage",
        "webNavigation"],
    "host_permissions": [
        "https://jira.spectr.dev/*"
    ],
    "background":{
        "service_worker": "backgroundPopapSize.js",
        "type": "module"
    }
}